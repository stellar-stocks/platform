{
  "name": "stellar-stocks",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "dotenv -e ../../.env.local -- turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "db:generate": "dotenv -- pnpm run -C packages/db db:generate",
    "db:migrate": "dotenv -- pnpm run -C packages/db db:migrate",
    "db:push": "dotenv -- pnpm run -C packages/db db:push",
    "db:studio": "dotenv -- pnpm run -C packages/db db:studio"
  },
  "devDependencies": {
    "prettier": "^3.7.4",
    "turbo": "^2.7.2",
    "typescript": "5.9.3"
  },
  "engines": {
    "node": ">=18"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "@noble/hashes": "1.1.0",
    "@scure/bip32": "1.1.0",
    "dotenv-cli": "^11.0.0",
    "iron-session": "^8.0.4",
    "next": "16.1.4"
  },
  "overrides": {
    "lucide-react": "^0.563.0",
    "@scure/bip32": "1.1.0",
    "@noble/hashes": "1.1.0"
  },
  "packageManager": "pnpm@10.28.1",
  "pnpm": {
    "overrides": {
      "next": "16.1.4",
      "lucide-react": "0.383.0",
      "@privy-io/react-auth": "^2.25.0",
      "react": "19.2.3",
      "react-dom": "19.2.3"
    }
  }
}
