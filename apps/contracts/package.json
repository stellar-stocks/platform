{
  "name": "contrarcts-tests",
  "version": "1.0.0",
  "description": "Run unit tests on this project.",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "nodemon --watch src --exec tsx src/index.ts",
    "start": "tsx src/index.ts",
    "test": "vitest run",
    "test:report": "vitest run -- --coverage --costs",
    "test:watch": "chokidar \"tests/**/*.ts\" \"contracts/**/*.clar\" -c \"npm run test:report\""
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@hirosystems/clarinet-sdk": "^3.0.2",
    "@pythnetwork/hermes-client": "^2.1.0",
    "@stacks/network": "^7.3.1",
    "@stacks/transactions": "^7.0.6",
    "chokidar-cli": "^3.0.0",
    "cors": "^2.8.6",
    "dotenv": "^16.6.1",
    "express": "^5.2.1",
    "node-cron": "^4.2.1",
    "vitest": "^3.1.3",
    "vitest-environment-clarinet": "^2.3.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node-cron": "^3.0.11",
    "nodemon": "^3.1.0",
    "tsx": "^4.7.1",
    "vite": "^7.3.1"
  }
}
